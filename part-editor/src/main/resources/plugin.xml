<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin id="org.digimead.tabuddy.desktop.editor" name="ModelEditor" version="0.0.1.0-SNAPSHOT" provider-name="Digimead team">
  <extension point="org.eclipse.ui.bindings">
    <key commandId="org.digimead.tabuddy.desktop.editor.ToggleIdentificators" contextId="org.eclipse.ui.contexts.window" sequence="M1+I" schemeId="org.digimead.tabuddy.desktop">
      </key>
    <key commandId="org.digimead.tabuddy.desktop.editor.ToggleEmpty" contextId="org.eclipse.ui.contexts.window" sequence="M1+E" schemeId="org.digimead.tabuddy.desktop">
      </key>
    <key commandId="org.digimead.tabuddy.desktop.editor.ExpandAll" contextId="org.eclipse.ui.contexts.window" sequence="M1+E" schemeId="org.digimead.tabuddy.desktop">
      </key>
    <key commandId="org.digimead.tabuddy.desktop.editor.CollapseAll" contextId="org.eclipse.ui.contexts.window" sequence="M1+C" schemeId="org.digimead.tabuddy.desktop">
      </key>
    <key commandId="org.digimead.tabuddy.desktop.editor.ElementNew" contextId="org.eclipse.ui.contexts.window" sequence="M1+N" schemeId="org.digimead.tabuddy.desktop">
      </key>
    <key commandId="org.digimead.tabuddy.desktop.editor.ElementEdit" contextId="org.eclipse.ui.contexts.window" sequence="M1+R" schemeId="org.digimead.tabuddy.desktop">
      </key>
    <key commandId="org.digimead.tabuddy.desktop.editor.ElementDelete" contextId="org.eclipse.ui.contexts.window" sequence="M1+D" schemeId="org.digimead.tabuddy.desktop">
      </key>
  </extension>
  <extension point="org.eclipse.ui.commands">
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.ToggleIdentificators" name="ToggleIdentificators"/>
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.ToggleEmpty" name="ToggleEmpty"/>
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.ExpandAll" name="ExpandAll"/>
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.CollapseAll" name="CollapseAll"/>
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.ElementNew" name="ElementNew"/>
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.ElementEdit" name="ElementEdit"/>
    <command categoryId="org.eclipse.ui.category.window" id="org.digimead.tabuddy.desktop.editor.ElementDelete" name="ElementDelete"/>
  </extension>
  <extension point="org.eclipse.ui.handlers">
    <handler class="org.digimead.tabuddy.desktop.editor.handler.ToggleIdentificators" commandId="org.digimead.tabuddy.desktop.editor.ToggleIdentificators">
      </handler>
    <handler class="org.digimead.tabuddy.desktop.editor.handler.ToggleEmpty" commandId="org.digimead.tabuddy.desktop.editor.ToggleEmpty">
      </handler>
    <handler class="org.digimead.tabuddy.desktop.editor.handler.ExpandAll" commandId="org.digimead.tabuddy.desktop.editor.ExpandAll">
      </handler>
    <handler class="org.digimead.tabuddy.desktop.editor.handler.CollapseAll" commandId="org.digimead.tabuddy.desktop.editor.CollapseAll">
      </handler>
    <handler class="org.digimead.tabuddy.desktop.editor.handler.ElementNew" commandId="org.digimead.tabuddy.desktop.editor.ElementNew">
      </handler>
    <handler class="org.digimead.tabuddy.desktop.editor.handler.ElementEdit" commandId="org.digimead.tabuddy.desktop.editor.ElementEdit">
      </handler>
    <handler class="org.digimead.tabuddy.desktop.editor.handler.ElementDelete" commandId="org.digimead.tabuddy.desktop.editor.ElementDelete">
      </handler>
  </extension>
  <extension point="org.eclipse.ui.perspectives">
    <perspective name="Model Perspective" class="org.digimead.tabuddy.desktop.editor.ModelPerspective" id="org.digimead.tabuddy.desktop.editor.ModelPerspective">
      </perspective>
  </extension>
  <extension point="org.eclipse.e4.workbench.model">
    <processor class="org.digimead.tabuddy.desktop.editor.Processor">
        </processor>
  </extension>
  <extension point="org.eclipse.ui.newWizards">
    <category name="%Category.TABuddy" id="org.digimead.TABuddy">
    </category>
    <wizard id="org.digimead.tabuddy.desktop.editor.wizard.NewModelWizard" name="%NewModelWizard.name" class="org.digimead.tabuddy.desktop.editor.wizard.ModelCreationWizard" category="org.digimead.TABuddy" icon="icons/16x16/document-new.png">
      <description>%NewModelWizard.desc</description>
      <selection class="org.eclipse.core.resources.IResource"/>
    </wizard>
  </extension>
  <extension point="org.eclipse.ui.menus">
    <menuContribution locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
      <toolbar id="org.digimead.tabuddy.desktop.editor.toolbar.EditorToolBar">
        <command
              commandId="org.digimead.tabuddy.desktop.editor.ToggleIdentificators"
              icon="icons/16x16/document-new.png"
              style="push">
           </command>
        <command
              commandId="org.digimead.tabuddy.desktop.editor.ToggleEmpty"
              icon="icons/16x16/document-new.png"
              style="push">
           </command>
        <command
              commandId="org.digimead.tabuddy.desktop.editor.ExpandAll"
              icon="icons/16x16/expand.png"
              style="push">
           </command>
        <command
              commandId="org.digimead.tabuddy.desktop.editor.CollapseAll"
              icon="icons/16x16/collapse.png"
              style="push">
           </command>
      </toolbar>
      <toolbar id="org.digimead.tabuddy.desktop.editor.toolbar.ElementToolBar">
        <command
              commandId="org.digimead.tabuddy.desktop.editor.ElementNew"
              icon="icons/16x16/document-new.png"
              style="push">
           </command>
        <command
              commandId="org.digimead.tabuddy.desktop.editor.ElementEdit"
              icon="icons/16x16/document-new.png"
              style="push">
           </command>
        <command
              commandId="org.digimead.tabuddy.desktop.editor.ElementDelete"
              icon="icons/16x16/expand.png"
              style="push">
           </command>
      </toolbar>
    </menuContribution>
  </extension>
</plugin>
