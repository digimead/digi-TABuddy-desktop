<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox spacing="5.0" xmlns:fx="http://javafx.com/fxml" fx:controller="org.digimead.tabuddy.desktop.dialog.ElementTemplateEditor$Controller">
  <children>
    <HBox alignment="CENTER_LEFT" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" VBox.vgrow="SOMETIMES">
      <children>
        <Label fx:id="label" text="Template name" />
        <TextField fx:id="name" prefWidth="-1.0" promptText="scope" HBox.hgrow="ALWAYS" />
      </children>
    </HBox>
    <HBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" VBox.vgrow="SOMETIMES">
      <children>
        <Button fx:id="add" mnemonicParsing="false" text="Add" />
        <Button fx:id="delete" mnemonicParsing="false" text="Delete" />
      </children>
    </HBox>
    <HBox alignment="TOP_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" VBox.vgrow="SOMETIMES">
      <children>
        <Button fx:id="ok" disable="true" mnemonicParsing="false" text="Ok" />
        <Button fx:id="cancel" cancelButton="true" defaultButton="true" mnemonicParsing="false" text="Cancel" />
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
</VBox>
